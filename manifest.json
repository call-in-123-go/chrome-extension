{
  "manifest_version": 3,
  "name": "Swift Reader",
  "version": "1.0",
  "description": "Remove ADS, add dark mode and definition function",
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/bird16.png",
      "48": "icons/bird48.png",
      "128": "icons/bird128.png"
    }
  },
  "icons": {
    "16": "icons/bird16.png",
    "48": "icons/bird48.png",
    "128": "icons/bird128.png"
  }
}
